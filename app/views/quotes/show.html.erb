
<div class="results">
  <p class="resultShow">
    <h2 class="priceHeader">Your Price:</h2>
    $<%= sql = "select ((0.33 * e.price * r.screen_risk) + 
          (0.2 * e.price * r.battery_risk) + (1 * e.price * r.total_loss_risk)) AS premium 
          from ex_models e join risk_by_brands r 
          on e.risk_by_brand_id=r.id 
          join quotes q ON q.ex_model_id=e.id 
          order by q.created_at desc limit 1;"
  results = ActiveRecord::Base.connection.exec_query(sql).cast_values
  JSON.pretty_generate(results) %>
  </p>


